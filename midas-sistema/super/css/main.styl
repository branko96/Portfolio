// CSS para todos en GENERAL

// CSS para la grafica de torta SEXO SOCIOS
.nav-date
    padding 10px 15px 5px 15px
    color #fff
    font-size 20px
    
.icon-M
    width 15px
    height 15px
    background #ACC9D7
    display inline-block
    margin-right 5px

.icon-F
    width 15px
    height 15px
    background #eed0f0
    display inline-block
    margin-right 5px

.caption
    padding 10px 
    width 100%
    font-size 11px
    text-align center

.productbox 
    position relative
    border 1px solid #ddd
    border-radius 4px
    -webkit-transition all .2s ease-in-out
    
figure 
    width 100%
    height 200px
    background-position center center
    background-size cover
    border 8px solid #fff
        

.productbox .wrap_canje
    padding 10px
    text-align center
    background #62718a
    color #FFF

.productbox .caption
    h5
        height 150px
        overflow hidden

.paginacion
    b,a
        border 1px solid #ddd 
        padding 5px 10px
        margin 2px
        color #8a6d3b
    a:hover
        background #eb9316;
        text-decoration none
        color #fff
  
  
h5 
  margin-top 0px
  max-height 30px
  overflow hidden
  
.wrapSocio
  margin-bottom 20px
  #existe
    margin-top 20px
    padding 20px
    background-color rgba(0, 0, 0, .10)
    width 100%
    min-height 100px !important
    border-radius 10px
    font-size 20px
    color rgba(0, 0, 0, 0.50)