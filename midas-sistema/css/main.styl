body
	color #333 !important
	
select#club
	margin 0px 10px 0px 0px
  

#login 
	width 200px
	padding 15px
	
.caption
	padding 10px 
	width 100%
	font-size 11px
	text-align center
	h5
		height 30px !important
		overflow hidden

.productbox 
	position relative
	border 1px solid #ddd
	border-radius 4px
	-webkit-transition all .2s ease-in-out
	
figure 
	width 100%
	height 200px
	background-position center center
	background-size cover
	border 8px solid #fff
	    

.productbox .wrap_canje
	padding 10px
	text-align center
	background #62718a
	color #FFF

.paginacion
	overflow hidden
	b,a
		border 1px solid #ddd 
		padding 5px 10px
		margin 2px
		color #8a6d3b
	a:hover
		background #eb9316;
		text-decoration none
		color #fff